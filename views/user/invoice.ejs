                   <section class="content-main">
                    <div class="content-header ">
                        <br>
                        <br>
                        <div>
                            <h2 class="content-title card-title ml-200">Invoice</h2>
                            <p class="ml-200">Details for Order ID: <%= order.date %></p>
                        </div>
                    </div>
                    <div class="row justify-content-center" >
                        <div class="col-lg-8 col-md-12">
                            <div class="card">
                                <header class="card-header">
                                    <div class="row align-items-center">
                                        <div class="col-lg-6 col-md-6 mb-lg-0 mb-15">
                                            <span>
                                                <i class="material-icons md-calendar_today"></i> <b></b>
                                            </span> <br>
                                            <% for (let i=0; i < order.length;i++) { %>
                                            <small class="text-muted">Order ID:<%= order.date %> </small>
                                        </div>
                                        <div class="col-lg-6 col-md-6 ms-auto text-md-end">                                   
                                            <a class="btn btn-primary" href="/saveinvoice?id=<%=order[i]._id %>"> <i class="icon material-icons md-cloud_download mr-5"></i>Save PDF</a>
        
                                        </div>
                                    </div>
                                </header> <!-- card-header end// -->
        
                                <br><br>
                                <div class="card-body">
                                    <div class="receipt-content">
                                        <div class="container bootstrap snippets bootdey">
                                            <div class="row">
                                                
                                                
                                                    <div class="col-md-12">
                                                        <div class="invoice-wrapper">
                                                            <div class="intro">
                                                                Hi <strong> <%= order[i].address[0].name %></strong>,
                                                                <br>
                                                                This is the receipt for a payment of <strong>₹<%= order[i].totalPrice %></strong>  for your works
                                                            </div>
                                                          
                                                            <div class="payment-info">
                                                                <div class="row">
                                                                    <div class="col-sm-6">
                                                                        <span>Payment No.</span>
                                                                        <strong></strong>
                                                                    </div>
                                                                    <div class="col-sm-6 text-end">
                                                                        <span>Payment Date</span>
                                                                        <strong><%=order[i].createdAt.toLocaleDateString()%></strong>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                             
                                                            <div class="payment-details">
                                                                <div class="row">
                                                                    <div class="col-sm-6">
                                                                        <span>Client:</span>
                                                                        <strong>
                                                                            <%= order[i].address[0].name %>
                                                                        </strong>
                                                                        <p>
                                                                            <%= order[i].address[0].state %> <br>
        
                                                                           <%= order[i].address[0].city %><br>
                                                                            <%= order[i].address[0].pinCode %><br>

                                                                             <br>
                                                                            <a href="#">
                                                                            
                                                                            </a>
                                                                        </p>
                                                                    </div>
                                                                    <div class="col-sm-6 text-end">
                                                                        <span>Payment To</span><br>
                                                                        <strong>
                                                                            <%= order[i].address[0].name %>
                                                                        </strong>
                                                                        <p>

                                                                            <%= order[i].address[0].state %><br>
                                                                           <%= order[i].address[0].landMark %>  <br>
                                                                           <%= order[i].address[0].pincode %><br>
                                                                             <br>
                                                                            <a href="#">
                                                                                
                                                                            </a>
                                                                        </p>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <%}%>
                                                            <div class="line-items">
                                                                <div class="headers clearfix">
                                                                    <div class="row">
                                                                        <div class="col-md-4">Description</div>
                                                                        <div class="col-md-3">Quantity</div>
                                                                        <div class="col-md-5 text-end">Amount</div>
                                                                    </div>
                                                                </div>
                                                                <%  order.forEach((item)=>{%>
                                                                  <%  item.products.forEach((items)=>{%> 

                                                                
                                                                    <div class="items">
                                                                        <div class="row item">
                                                                            <div class="col-md-4 desc">
                                                                                <%= items.product.productName %>     
                                                                            </div>
                                                                           
                                                                            <div class="col-md-3 qty">
                                                                                <%= items.quantity %>  
                                                                            </div>
                                                                            <div class="col-md-5 amount text-end">
                                                                                ₹  <%= items.product.salePrice %> 
        
                                                                            </div>
                                                                        </div>
                                                                        <%}) %>
                                                                       
                                                                    
        
                                                                       
                                                                    
                                                                </div>
                                                                <br><br><br><br><br>
                                                                <div class="total text-end">
                                                                    <!-- <p class="extra-notes">
                                                                        
                                                                    
                                                                        Thanks a lot.
                                                                    </p> -->
                                                                    <div class="field">
                                                                        Subtotal <span> </span>
                                                                    </div>
                                                                    <div class="field">
                                                                        
                                                                    </div>
                                                                   
                                                                    <div class="field grand-total">
                                                                        Total <span>₹<%= item.totalPrice %></span>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <%}) %>
                                                        <div class="footer">
                                                            @evara
                                                        </div>
                                                    </div>
                                                
                                            </div>
                                        </div>
                                    </div> 
                                </div> 
        
                                
                            </div>
                        </div>
                    </div>
                </section> 
                <br><br><br><br><br><br><br>



