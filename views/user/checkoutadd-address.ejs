                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-9">
                        <label for="city">City:</label>
                        <input type="text" class="form-control" id="city" name="city" required>
                        <span id="cityError" class="error-message"></span>
                    </div>
                    <div class="form-group col-md-9">
                        <label for="landMark">Landmark:</label>
                        <input type="text" class="form-control" id="landMark" name="landMark" required>
                        <span id="landMarkError" class="error-message"></span>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <label for="state">State:</label>
                        <input type="text" class="form-control" id="state" name="state" required>
                        <span id="stateError" class="error-message"></span> <!-- Change id to 'stateError' -->
                    </div>
                    <div class="form-group col-md-6">
                        <label for="pincode">Pincode:</label>
                        <input type="number" class="form-control" id="pincode" name="pincode" required>
                        <span id="pincodeError" class="error-message"></span>
                    </div>
                    <div class="form-group col-md-6">
                        <label for="phone">Phone:</label>
                        <input type="number" class="form-control" id="phone" name="phone" required>
                        <span id="phoneError" class="error-message"></span>
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col-md-6 ">
                        <label for="altPhone">Alternate Phone:</label>
                        <input type="number" class="form-control mx-auto" id="altPhone" name="altPhone" required>
                        <span id="altPhoneError" class="error-message"></span> <!-- Change id to 'altPhoneError' -->
                    </div>
                </div>
                <div class="text-center">
                    <button type="button" id="submitButton" class="btn btn-primary" onclick="saveAddress()">Save</button>
     
                </div>
            </form>
        </div>
    </div>
</div>
<!-- ................................validation -->
<script>
function validateForm() {
    const addressType = document.getElementById('addressType').value.trim();
    const name = document.getElementById('name').value.trim();
    const city = document.getElementById('city').value.trim();
    const landMark = document.getElementById('landMark').value.trim();
    const state = document.getElementById('state').value.trim();
    const pincode = document.getElementById('pincode').value.trim(); 
    const phone = document.getElementById('phone').value.trim(); 
    const altPhone = document.getElementById('altPhone').value.trim(); 
    
    // Define regex patterns
    const namePattern = /^[a-zA-Z]+(?:\s[a-zA-Z]+)*(?:\s[a-zA-Z]+)?$/; 
    const addressTypePattern = /^[a-zA-Z]+(?:\s[a-zA-Z]+)*(?:\s[a-zA-Z]+)?$/; 
    const cityPattern = /^[a-zA-Z]+(?:\s[a-zA-Z]+)*(?:\s[a-zA-Z]+)?$/; 
    const landMarkPattern = /^[A-Za-z0-9]+(?:\s[A-Za-z0-9]+)*$/; 
    const statePattern = /^[a-zA-Z]+$/; 
    const pincodePattern = /^\d{6}$/; 
    const phonePattern = /^\d{10}$/; 
    const altPhonePattern = /^\d{10}$/; 
    
    // Validate each field
    let isValid = true;
    if (!namePattern.test(name)) {
        document.getElementById('nameError').textContent = 'Please enter a valid name (letters only).';
        isValid = false;
    } else {
        document.getElementById('nameError').textContent = '';
    }
    if (!addressTypePattern.test(addressType)) {
        document.getElementById('addressTypeError').textContent = 'Please enter a valid address type (letters and spaces only).';
        isValid = false;
    } else {
        document.getElementById('addressTypeError').textContent = '';
    }
    if (!cityPattern.test(city)) {
        document.getElementById('cityError').textContent = 'Please enter a valid city (letters and spaces only).';
        isValid = false;
    } else {
        document.getElementById('cityError').textContent = '';
    }
    if (!landMarkPattern.test(landMark)) {
        document.getElementById('landMarkError').textContent = 'Please enter a valid landmark (letters and spaces only).';
        isValid = false;
    } else {
        document.getElementById('landMarkError').textContent = '';
    }
    if (!statePattern.test(state)) {
        document.getElementById('stateError').textContent = 'Please enter a valid state (letters and spaces only).';
        isValid = false;
    } else {
        document.getElementById('stateError').textContent = '';
    }
    if (!pincodePattern.test(pincode)) {
        document.getElementById('pincodeError').textContent = 'Please enter a valid pincode (exactly 6 digits).';
        isValid = false;
    } else {
        document.getElementById('pincodeError').textContent = '';
    }
    if (!phonePattern.test(phone)) {
        document.getElementById('phoneError').textContent = 'Please enter a valid phone number (exactly 10 digits).';
        isValid = false;
    } else {
        document.getElementById('phoneError').textContent = '';
    }
    if (!altPhonePattern.test(altPhone)) {
        document.getElementById('altPhoneError').textContent = 'Please enter a valid alternate phone number (exactly 10 digits).';
        isValid = false;
    } else {
        document.getElementById('altPhoneError').textContent = '';
    }

    return isValid;
}

function saveAddress() {
    if (validateForm()) {
        // If all validations pass, proceed with saving the address
        document.getElementById('addressForm').submit();
    }
}

</script>








