<script>
    function addToCart(prodId) {
        var quantity = 1;
        
        $.ajax({
            url: `/addtoCart`,
            method: "post",
            data: {
                quantity: quantity,
                id:prodId
            },
            success: (res) => {
                console.log(res,"res");
                if (res.status === true) {
                    
                    Swal.fire({
                        title: 'Added to Cart!',
                        text: 'Continue Shopping',
                        icon: 'success',
                        timer: 2000,
                        showConfirmButton: false
                    });
                
                } else if (res.status === "out of stock") {
                            Swal.fire({
                                title: 'Stock!!',
                                text: "The product is out of stock",
                                icon: 'error', // Corrected from 'fail' to 'error'
                                timer: 2000,
                            })
                        }
                  else {
                    
                    Swal.fire({
                        title: 'You need to Login first',
                        text: 'Continue Shopping',
                        icon: 'warning',
                        timer: 2000,
                        showConfirmButton: false
                    }).then(() => { window.location.href = '/login' });
                }
            },
            error: (err) => {
            
                Swal.fire({
                    title: 'Error!',
                    text: 'An error occurred while processing your request.',
                    icon: 'error',
                    timer: 2000,
                });
            }
        });
    }
</script>



